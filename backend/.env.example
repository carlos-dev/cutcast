# Webhook URL do n8n para processar vídeos
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/process-video

# Database URLs para Prisma
DATABASE_URL="postgresql://user:password@host:port/database?schema=public"
DIRECT_URL="postgresql://user:password@host:port/database"

# Supabase credentials (apenas para database, não para storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key-here
SUPABASE_PASSWORD=your-database-password

# Cloudflare R2 Storage (para armazenamento de vídeos)
R2_S3_API=your-s3-api-here
R2_ACCESS_KEY_ID=your-access-key-id-here
R2_SECRET_ACCESS_KEY=your-secret-access-key-here
R2_BUCKET_NAME=videos
R2_PUBLIC_DOMAIN=https://pub-xxxxxxxxxxxxx.r2.dev
